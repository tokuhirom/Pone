start	= abcd+

abcd	= 'a' { printf("A %d\n", G->pos); } bc { printf("ABC %d\n", G->pos); } &{YYACCEPT}
	 | 'b' { printf("B %d\n", G->pos); } cd { printf("BCD %d\n", G->pos); } &{YYACCEPT}

bc	= 'b' { printf("B %d\n", G->pos); } 'c' { printf("C %d\n", G->pos); }

cd	= 'c' { printf("C %d\n", G->pos); } 'd' { printf("D %d\n", G->pos); }
